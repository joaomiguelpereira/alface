<process-definition  xmlns="urn:jbpm.org:jpdl-3.2"  name="simple">


	<start-state name="start">
		<transition to="do something"></transition>
	</start-state>


	<state name="do something">
		<transition to="end"></transition>
	</state>


	<end-state name="end"></end-state>


</process-definition>